{
  "expo": {
    "name": "共好 APP",
    "slug": "hospital-allgood",
    "scheme": "app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/images/logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png",
      "backgroundColor": "#222"
    },
    "experiments": {
      "reactServerFunctions": false
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hospital.allgood",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#222"
      },
      "package": "com.hospital.allgood",
      "googleServicesFile": "./google-services.json",
      "orientation": "default",
      "softwareKeyboardLayoutMode": "pan",
      "useNextNotificationsApi": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./src/assets/images/logo.png",
      "output": "static",
      "bundler": "metro"
    },
    "androidStatusBar": {
      "backgroundColor": "#000",
      "translucent": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "defaultChannel": "allgood-info",
          "color": "#ffffff",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-router",
      "expo-font",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      ["expo-screen-orientation", { "initialOrientation": "DEFAULT" }]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "861dff78-8223-47e5-b92c-45d4f6ef4bdf"
      },
      "owner": "nickfury95"
    }
  }
}